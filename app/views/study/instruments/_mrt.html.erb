		else $('#begin_button').button("option", "disabled", true);
		}

		// hide the current page and change its page number to be standard
		// and show the next page and change its page number to be highlighted
		function show_and_hide (page_to_hide, page_to_show, old_page_number, new_page_number) {
		document.getElementById(page_to_hide).style.display = "none";
		document.getElementById(page_to_show).style.display = "block";
		document.getElementById(old_page_number).className = "";
		document.getElementById(new_page_number).className = "selected_page";
		}

		// when user advances from instructions page, start the timer going
		// and then submit the overall form, with all of the 10 or 20 questions,
		// when the time is up
		function start_timer (part) {
		three_minutes = 3 * 60 * 1000;
		if (part == 1) {
			timer = setTimeout("end_part1_and_continue()", three_minutes);
		}
		else if (part == 2) {
			timer = setTimeout("end_part2_and_stop()", three_minutes);
		}
		}
		
		function clear_timer () {
			clearTimeout(timer);
		}

		function end_part1_and_continue() {
		show_and_hide("mrt_part_1", "part1_complete", "page_number_part1", "page_number_part1_complete");
		}

		function end_part2_and_stop() {
		show_and_hide("mrt_part_2", "part2_complete", "page_number_part2", "page_number_part2_complete");
		}
		</script>
		
