		function practice_check () {
		var complete = new Boolean();
		complete = true;

		// practice 1: first and second drawings are correct
		if (!document.mrt_form.mrt_qinst4_a.checked) document.getElementById("inst4a").style.background = "#FF706C";
		else document.getElementById("inst4a").style.background = "#FFFFFF";
		if (!document.mrt_form.mrt_qinst4_b.checked) document.getElementById("inst4b").style.background = "#FF706C";
		else document.getElementById("inst4b").style.background = "#FFFFFF";
		if (document.mrt_form.mrt_qinst4_c.checked) document.getElementById("inst4c").style.background = "#FF706C";
		else document.getElementById("inst4c").style.background = "#FFFFFF";
		if (document.mrt_form.mrt_qinst4_d.checked) document.getElementById("inst4d").style.background = "#FF706C";
		else document.getElementById("inst4d").style.background = "#FFFFFF";

		if (document.mrt_form.mrt_qinst4_a.checked
			&& document.mrt_form.mrt_qinst4_b.checked
			&& !(document.mrt_form.mrt_qinst4_c.checked)
			&& !(document.mrt_form.mrt_qinst4_d.checked)) {
			document.getElementById("mrt_qinst4_incorrect").style.display = "none";
			document.getElementById("mrt_qinst4_correct").style.display = "block";
		}
		else {
			document.getElementById("mrt_qinst4_correct").style.display = "none";
			document.getElementById("mrt_qinst4_incorrect").style.display = "block";
			complete = false;
		}

		// practice 2: first and third drawings are correct
		if (!document.mrt_form.mrt_qinst5_a.checked) document.getElementById("inst5a").style.background = "#FF706C";
		else document.getElementById("inst5a").style.background = "#FFFFFF";
		if (document.mrt_form.mrt_qinst5_b.checked) document.getElementById("inst5b").style.background = "#FF706C";
		else document.getElementById("inst5b").style.background = "#FFFFFF";
		if (!document.mrt_form.mrt_qinst5_c.checked) document.getElementById("inst5c").style.background = "#FF706C";
		else document.getElementById("inst5c").style.background = "#FFFFFF";
		if (document.mrt_form.mrt_qinst5_d.checked) document.getElementById("inst5d").style.background = "#FF706C";
		else document.getElementById("inst5d").style.background = "#FFFFFF";

		if (document.mrt_form.mrt_qinst5_a.checked
			&& !(document.mrt_form.mrt_qinst5_b.checked)
			&& document.mrt_form.mrt_qinst5_c.checked
			&& !(document.mrt_form.mrt_qinst5_d.checked)) {
			document.getElementById("mrt_qinst5_incorrect").style.display = "none";
			document.getElementById("mrt_qinst5_correct").style.display = "block";
		}
		else {
			document.getElementById("mrt_qinst5_correct").style.display = "none";
			document.getElementById("mrt_qinst5_incorrect").style.display = "block";
			complete = false;
		}

		// practice 3: second and third drawings are correct
		if (document.mrt_form.mrt_qinst6_a.checked) document.getElementById("inst6a").style.background = "#FF706C";
		else document.getElementById("inst6a").style.background = "#FFFFFF";
		if (!document.mrt_form.mrt_qinst6_b.checked) document.getElementById("inst6b").style.background = "#FF706C";
		else document.getElementById("inst6b").style.background = "#FFFFFF";
		if (!document.mrt_form.mrt_qinst6_c.checked) document.getElementById("inst6c").style.background = "#FF706C";
		else document.getElementById("inst6c").style.background = "#FFFFFF";
		if (document.mrt_form.mrt_qinst6_d.checked) document.getElementById("inst6d").style.background = "#FF706C";
		else document.getElementById("inst6d").style.background = "#FFFFFF";

		if (!(document.mrt_form.mrt_qinst6_a.checked)
			&& document.mrt_form.mrt_qinst6_b.checked
			&& document.mrt_form.mrt_qinst6_c.checked
			&& !(document.mrt_form.mrt_qinst6_d.checked)) {
			document.getElementById("mrt_qinst6_incorrect").style.display = "none";
			document.getElementById("mrt_qinst6_correct").style.display = "block";
		}
		else {
			document.getElementById("mrt_qinst6_correct").style.display = "none";
			document.getElementById("mrt_qinst6_incorrect").style.display = "block";
